		link(rel='stylesheet', href='../assets/css/datepicker.css')
		link(rel='stylesheet', href='../assets/css/select2.css')
		link(rel='stylesheet', href='../assets/css/custom.css')
		link(rel='stylesheet', href='../assets/css/print.css')
		style
			body { padding-top: 0px }


	body
		.btn-group
			a.btn.btn-primary(href="#", data-bind="click: toExcel") Экспорт в Excel
			// ko if: exported
			a.btn(data-bind="attr: {href: exportHref}", download="print.xls") Скачать
			// /ko
		table.table.table-striped#toprint
			thead
				tr
					th Номер
					th ФИО
					th Научная степень
					th Ученое звание
					th Должность
					th Место работы
					th Город
					th Страна
					th Почтовый адрес
					th Электронный адрес
					th Телефон
					th Форма участия в конференции
					th Название доклада
					th Номер секции
					th Участие в монографии
					th Название раздела монографии
					th Потребность в проживании
					th Оплата тезисов безнал
					th Оплата тезисов наличные
					th Сумма к оплате монографии
					th Оплата монографии безнал
					th Оплата монографии наличные
					th Оргвзнос (очное)
					th Отметка об отправке материалов
			tbody(data-bind="foreach: users")
				tr
					td
						div(data-bind="text: id")
					td
						div(data-bind="text: fullName")
					td
						div(data-bind="text: academicDegree")
					td
						div(data-bind="text: academicTitle")
					td
						div(data-bind="text: jobPosition")
					td
						div(data-bind="text: jobPlace")
					td
						div(data-bind="text: city")
					td
						div(data-bind="text: country")
					td
						div(data-bind="text: postalAddress")
					td
						div(data-bind="text: email")
					td
						div(data-bind="text: phone")
					td
						div(data-bind="text: participantType")
					td
						div(data-bind="text: lectureTitle")
					td
						div(data-bind="text: sectionNumber")
					td
						div(data-bind="checkbox: function(){ return monographyParticipant === '1'; }")
					td
						div(data-bind="text: monographyTitle")
					td
						div(data-bind="text: stayPeriod")
					td 
						div(data-bind="text: z_thesisPay")
					td 
						div -
					td 
						div(data-bind="text: z_monographyPay")
					td 
						div(data-bind="text: z_monographyPay")
					td 
						div -
					td 
						div(data-bind="text: z_organizationPay")
					td
						div(data-bind="checkbox: function(){ return true; }")

